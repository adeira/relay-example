[ignore]

[untyped]

[declarations]
.+/node_modules/.+

[include]

[libs]
./flow-typed/

[lints]
all=error
ambiguous-object-type=off
dynamic-export=off
sketchy-null-bool=off
unclear-type=off
untyped-import=off

[strict]
ambiguous-object-type
sketchy-null-bool
unclear-type
untyped-import

[options]
include_warnings=true

; Allow require(path.join(__dirname, 'myFile.json'))
module.ignore_non_literal_requires=true

esproposal.optional_chaining=enable
esproposal.nullish_coalescing=enable
